{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": [
      "next-env.d.ts"
    ],
    "baseUrl": "src/",
    "paths": {
      "@speckle-viewer": [
        "../../../libs/speckle-viewer/src/index.ts"
      ],
      "#/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../apps/fe/.next/types/**/*.ts",
    "../../dist/apps/fe/.next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
